// Comandos de dirección.

der = keyboard_check(vk_right);
izq = keyboard_check(vk_left);
arr = keyboard_check_pressed(vk_up);
abj = keyboard_check(vk_down);

// Acción de la gravedad.

if (!place_meeting(x,y+5,objM)) {
t2 = current_time;
tiempo = (t2-t1)/50;
y = y+(grav*tiempo);
}

// Chocar hacia abajo.

if (place_meeting(x,y+5,objM)) {
abj = false;
t1 = current_time ;
show_debug_message(t1);
}

// No chocar hacia los lados.

if (place_meeting(x+vel,y,objM)) {
der = false
}

if (place_meeting(x-vel,y,objM)) {
izq = false
}

if (der) {
    if (!place_meeting(x+5,y,objM)){
        x = x+vel
        }
}

if (izq) {
    if (!place_meeting(x-5,y,objM)){
    x = x-vel
    }
}

// Saltar solamente desde una plataforma

if (arr) {
    if (place_meeting(x,y+16,objM)){
    y = y-hvel;
    }
}

if (abj) {
y = y+vel
}

// No chocar hacia arriba.

if (place_meeting(x,y-17,objM)) {
y = y-16
}
</string>
